{
    "env": {
        "browser": true,
        "es2021": true
    },
	"parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
		"babelOptions": {
			"presets": ["@babel/preset-react"]
		},
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		
		"semi": "off",
		"strict": 0
    }
    }

